{
  "appId": "com.richdale.imageslider",
  "appName": "Image Slider",
  "webDir": "www",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#4A90E2",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP"
    },
    "Camera": {
      "presentationStyle": "fullscreen"
    }
  }
}
